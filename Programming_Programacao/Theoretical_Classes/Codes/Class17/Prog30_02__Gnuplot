set term x11 
set macros
style1 = "lines lt 3 lw 2 title \"Metodo de Euler\""
style2 = "lines lt 2 lw 2 title \"Metodo de Runge-Kutta 2\""
style3 = "lines lt 1 lw 2 title \"Solucao Exacta\""
range1 = "using 1:2"
range2 = "using 1:3"
range3 = "using 1:4"

plot "Prog30_02__Data.txt" @range1 with @style1, "Prog30_02__Data.txt" @range2 with @style2, "Prog30_02__Data.txt" @range3 with @style3

